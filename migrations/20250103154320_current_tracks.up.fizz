create_table("current_tracks") {
    t.Column("id", "integer", {primary: true})
    t.Column("name", "string")
    t.Column("length", "integer")
    t.Column("number_corners", "integer")
    t.Column("number_straights", "integer")
    t.Column("number_drs_zones", "integer")
    t.Column("year", "integer")
    t.Column("country", "string")
}